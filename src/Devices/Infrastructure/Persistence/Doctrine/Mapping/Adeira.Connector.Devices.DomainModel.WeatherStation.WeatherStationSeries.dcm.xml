<doctrine-mapping
		xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
							https://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

	<entity name="Adeira\Connector\Devices\DomainModel\WeatherStation\WeatherStationSeries" table="weather_stations_series">

		<unique-constraints>
			<unique-constraint columns="code" name="weather_stations_series_code_uindex"/>
		</unique-constraints>

		<id name="id" type="WeatherStationSeriesId">
			<generator strategy="NONE"/>
		</id>

		<field name="code" column="code" type="string"/>

	</entity>

</doctrine-mapping>
