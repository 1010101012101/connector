<doctrine-mapping
		xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
							https://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

	<entity name="Adeira\Connector\Inbound\DomainModel\DataSourceRecord\DataSourceRecord" table="data_sources_records">

		<id name="id" type="uuid">
			<generator strategy="NONE"/>
		</id>

		<field name="dataSourceId" column="data_source_id" type="uuid"/>

		<field name="data" column="data" type="jsonb"/>

	</entity>

</doctrine-mapping>
