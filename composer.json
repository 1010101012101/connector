{
  "name": "adeira/connector",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "adeira/compiler-extension": "^2.0",
    "boldtrn/jsonb-bundle": "^1.1",
    "doctrine/orm": "^2.5",
    "firebase/php-jwt": "^4.0",
    "latte/latte": "^2.4",
    "nette/application": "^v2.4",
    "nette/bootstrap": "^2.4 || ^3.0@dev",
    "nette/caching": "^2.5 || ^3.0@dev",
    "nette/di": "^2.4 || ^3.0@dev",
    "nette/http": "^v2.4 || ^3.0@dev",
    "nette/security": "^v2.4",
    "nette/utils": "3.0.x-dev as v2.4",
    "nextras/migrations": "^3.0",
    "ramsey/uuid": "^3.5",
    "ramsey/uuid-doctrine": "^1.2",
    "roave/security-advisories": "dev-master",
    "tracy/tracy": "^2.4",
    "webonyx/graphql-php": "^0.9.0"
  },
  "require-dev": {
    "jakub-onderka/php-parallel-lint": "^0.9.2",
    "mockery/mockery": "^0.9.7",
    "mrtnzlml/testbench": "dev-master",
    "nette/tester": "2.0.x-dev as v1.7",
    "phpstan/phpstan": "dev-master",
    "slevomat/coding-standard": "dev-php7"
  },
  "autoload": {
    "psr-4": {
      "Adeira\\Connector\\": [
        "extensions/",
        "src/"
      ]
    },
    "files": [
      "extensions/GraphQL/Types.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Adeira\\Connector\\Tests\\": [
        "tests/utils/"
      ]
    }
  },
  "scripts": {
    "adeira-connector-scripts": [
      "chmod +x bin/console",
      "chmod +x tests/run"
    ],
    "post-install-cmd": [
      "@adeira-connector-scripts"
    ],
    "post-update-cmd": [
      "@adeira-connector-scripts"
    ]
  },
  "minimum-stability": "stable"
}
