{
  "name": "adeira/connector",
  "type": "project",
  "license": "MIT",
  "require": {
    "adeira/compiler-extension": "^1.0",
    "adeira/rest-api": "dev-master",
    "boldtrn/jsonb-bundle": "^1.1",
    "doctrine/orm": "^2.5",
    "latte/latte": "^2.4",
    "nette/application": "^2.4",
    "nette/bootstrap": "^2.4",
    "nette/caching": "^2.5",
    "nette/di": "^2.4",
    "nette/forms": "^2.4",
    "nette/http": "^2.4",
    "nette/security": "^2.4",
    "nette/utils": "^2.4",
    "ramsey/uuid": "^3.5",
    "ramsey/uuid-doctrine": "^1.2",
    "roave/security-advisories": "dev-master",
    "tracy/tracy": "^2.4",
    "webonyx/graphql-php": "^0.7.2"
  },
  "require-dev": {
    "adeira/code-quality": "^1.0",
    "mrtnzlml/testbench": "^2.3",
    "nette/tester": "2.0.x-dev as v1.7"
  },
  "autoload": {
    "psr-4": {
      "Adeira\\Connector\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Adeira\\Connector\\Tests\\": [
        "tests/utils/"
      ]
    }
  },
  "scripts": {
    "adeira-connector-scripts": [
      "chmod +x bin/console"
    ],
    "post-install-cmd": [
      "@adeira-connector-scripts"
    ],
    "post-update-cmd": [
      "@adeira-connector-scripts"
    ]
  }
}
